/**
 * Retrieve the list of available schema.
 *
 * @return array
 */
function pno_get_properties_list() {

	$available_properties = [
	{% for property in properties %}
	'{{ property.name }}' => [
		'url' => '{{ property.resource }}',
		'types' => [
			{% for type in property.types %}
				'{{type}}',
			{% endfor %}
		],
	],
	{% endfor %}
	];

	return $available_properties;

}

