{
	"name": "pressmodo/posterno-schema",
	"description": "Pressmodo Scaffold",
	"type": "wordpress-plugin",
	"keywords": [
		"wordpress"
	],
	"homepage": "https://pressmodo.com",
	"authors": [{
		"name": "Pressmodo",
		"email": "hello@pressmodo.com"
	}],
	"require": {
		"php": ">=5.6",
		"composer/installers": "~1.0"
	},
	"autoload": {
		"psr-4": {
			"PNO\\": "includes/classes/",
			"Posterno\\SchemaOrg\\": "src"
		}
	},
	"autoload-dev": {
        "psr-4": {
            "Posterno\\SchemaOrg\\Generator\\": "generator"
        }
    },
	"require-dev": {
		"dealerdirect/phpcodesniffer-composer-installer": "0.4.4",
		"pressmodo/phpcs-config": "dev-master",
		"graham-campbell/analyzer": "^1.1",
        "larapack/dd": "^1.1",
        "league/flysystem": "^1.0",
        "phpunit/phpunit": "^6.0",
        "scrutinizer/ocular": "^1.5",
        "symfony/console": "^3.2",
        "symfony/css-selector": "^3.2",
        "symfony/dom-crawler": "^3.2",
        "twig/twig": "^1.28"
	},
	"scripts": {
		"lint": "phpcs .",
		"lint-fix": "phpcbf .",
		"generate": "php ./generate.php generate"
	},
	"config": {
        "sort-packages": true
	}
}
